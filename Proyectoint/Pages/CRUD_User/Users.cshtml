@page
@model Proyectoint.UsersModel

@{
    ViewData["Title"] = "Usuario";
}

<h1>Usuarios</h1>

<p>
    <a asp-page="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Usuarios[0].Nombre_Usuario)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Usuarios[0].Contraseña)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.Usuarios) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Nombre_Usuario)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Contraseña)
            </td>
            <td>
                <a asp-page="./EditUsers" asp-route-id="@item.Id_usuario">EditUsers</a> |
                <a asp-page="./Details" asp-route-id="@item.Id_usuario">Details</a> |
                <a asp-page="./Delete" asp-route-id="@item.Id_usuario">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
