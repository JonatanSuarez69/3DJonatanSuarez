@page

@model Proyectoint.Pages.InicioModel
@{
    ViewData["Title"] = "Inicio";
}

<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8" />
    <title>Login</title>

    <link href="~/lib/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/css/site.css" rel="stylesheet" />


</head>
<body>
    <div class="login-page">
        <div class="form">
            <form class="login-form">
                <label>Usuario</label>
                <input id="txtUsuario" type="text" placeholder="Nombre de usuario" />
                <label>Contraseña</label>
                <input id="txtContraseña" type="password" placeholder="Contraseña" />
                <button type="button" class="btn-success" id="btnEntrar">Entrar</button>
            </form>

        </div>

    </div>
</body>
</html>